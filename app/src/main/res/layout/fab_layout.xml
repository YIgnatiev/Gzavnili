<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/fab_delivery_request"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:elevation="20dp"
        android:src="@drawable/fab_delivery_request"
        android:visibility="visible"
        />


    <ImageView
        android:id="@+id/fab_pay"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:elevation="20dp"
        android:src="@drawable/fab_pay"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/fab_edit"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:elevation="20dp"
        android:src="@drawable/fab_edit"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/fab_delete"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/fab_delete"
        android:elevation="20dp"
        android:visibility="invisible"
         />
    <ImageView
        android:id="@+id/fab_share"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/fab_share"
        android:elevation="20dp"
        android:visibility="invisible"
       />
    <ImageView
        android:id="@+id/fab_tracking"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/fab_tracking"
        android:elevation="20dp"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/fab_show_proof"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:elevation="20dp"
        android:src="@drawable/fab_proof_of_delivery"
        android:visibility="invisible"
        />

    <ImageView
        android:id="@+id/fab_archive"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/fab_archive"
        android:elevation="20dp"
        android:visibility="invisible"
        />

    <ImageView
        android:id="@+id/fab_help"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/ic_fab_help"
        android:elevation="20dp"
        android:visibility="invisible"
        />
</FrameLayout>